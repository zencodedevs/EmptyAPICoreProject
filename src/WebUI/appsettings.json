{
  "UseInMemoryDatabase": false,
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=ZenAchitecture;Trusted_Connection=True;MultipleActiveResultSets=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace"
    }
  },
  "EndPoints": {
    "WebsiteURL": "https://localhost:5001"
  },
  "IdentityServer": {
    "Clients": {
      "ZenAchitecture.WebUI": {
        "Profile": "IdentityServerSPA",
        "RedirectUri": "https://localhost:4200/authentication/login-callback",
        "LogoutUri": "https://localhost:4200/authentication/logout-callback"
      }
    },
    "Key": {
      "Type": "Development"
    }
  },
  "Quartz": {
    "EventProcessorQuartezService": "0/15 * * * * ?",
    "EventProcessorCleanerQuartezService": "0 0 12 1 1/1 ? *"
  },
  "FeatureManagement": {
    "EventProcessorQuartezService": true,
    "EventProcessorCleanerQuartezService": true
  },
  "AllowedHosts": "*"


}
